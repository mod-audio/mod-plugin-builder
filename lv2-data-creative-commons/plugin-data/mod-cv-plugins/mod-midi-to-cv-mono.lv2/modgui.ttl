@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mod-devel/midi-to-cv-mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-midi-to-cv-mono.html> ;
        modgui:stylesheet <modgui/stylesheet-midi-to-cv-mono.css> ;
        modgui:screenshot <modgui/mod_midi_to_cv_mono_ss.png> ;
        modgui:thumbnail <modgui/mod_midi_to_cv_mono_tn.png> ;
        modgui:brand "MOD" ;
        modgui:license "CC BY-NC-ND 4.0";
        modgui:label "MIDI to CV mono" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Octave" ;
            lv2:name "Octave" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Semitone" ;
            lv2:name "Semitone" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "Cent" ;
            lv2:name "Cent" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "Retrigger" ;
            lv2:name "Retrigger" ;
        ] ;
    ] .
