@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mod-devel/mod-cv-control>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-control-to-cv.html> ;
        modgui:stylesheet <modgui/stylesheet-control-to-cv.css> ;
        modgui:screenshot <modgui/mod_control_to_cv_ss.png> ;
        modgui:thumbnail <modgui/mod_control_to_cv_tn.png> ;
        modgui:brand "MOD" ;
        modgui:license "CC BY-NC-ND 4.0";
        modgui:label "Control to CV" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Control" ;
            lv2:name "Control" ;
        ] ;
    ] .
