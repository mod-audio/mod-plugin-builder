@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://moddevices.com/plugins/mod-devel/mod-cv-switch2>
a lv2:Plugin, mod:ControlVoltagePlugin;

doap:name "CV Switchbox 2-1";

doap:developer [
    foaf:name "Jarno Verheesen";
    foaf:homepage <>;
    foaf:mbox <mailto:j.verheesen@hotmail.nl>;
    ];

doap:maintainer [
    foaf:name "MOD";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:jarno@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 0;

rdfs:comment """
CV Switchbox 2-1 routes either one of the two cv inputs to one cv output. 
""";

lv2:port
[
    a lv2:InputPort, lv2:CVPort;
    lv2:index 0;
    lv2:symbol "CVin1";
    lv2:name "CV in1";
],
[
    a lv2:InputPort, lv2:CVPort;
    lv2:index 1;
    lv2:symbol "CVin2";
    lv2:name "CV in2";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 2;
    lv2:symbol "CVout";
    lv2:name "CV out";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "Switch";
    lv2:name "Channel";
    lv2:portProperty lv2:integer;
    lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "1"; rdf:value 0];
        lv2:scalePoint [rdfs:label "2"; rdf:value 1];
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
]
.
